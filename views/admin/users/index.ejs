<%- include('../../partials/header.ejs'); %>
<%- include('../../partials/navbar.ejs'); %>

<div class="container">
    <hr>
    <h2>Usuarios</h2><br>
    <a class="btn btn-success" href="/admin/users/create">Criar Usuario</a>
    <hr>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>Login</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(function(temp){%>
            <tr>
                <td><%= temp.id %></td>
                <td><%= temp.login %></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</div>

<%- include('../../partials/footer.ejs') %>